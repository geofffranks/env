# WARNING! THIS FILE IS MAINTAINED BY CLOCKWORK! ANY CHANGES WILL BE OVERWRITTEN, AND QUICKLY!
# .gitconfig
[user]
	email = geoff.franks@gmail.com
	name = Geoff Franks
[ui]
	color = auto
[color]
	ui = true
[alias]
	br = branch
	co = checkout
	ci = commit
	who = shortlog -s --
	last = cat-file commit HEAD
	ds = diff --stat --name-status
	dsc = diff --stat --name-status --cached
	up = "!git remote update ; git gc"
	lc = log ORIG_HEAD.. --stat --no-merges
	undo = reset --hard
	ss = stash save
	sp = stash pop
	sls = stash list
[diff]
	patience = true
